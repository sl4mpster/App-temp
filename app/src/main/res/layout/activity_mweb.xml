<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".mweb">

    <WebView
    android:id="@+id/webview1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_below="@+id/buttonvg"
    >
    </WebView>

    <WebView
        android:id="@+id/webview2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/buttonvg">
    </WebView>

    <WebView
        android:id="@+id/webview3"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/buttonvg">
    </WebView>

    <Button
        android:layout_width="330px"
        android:layout_height="200px"
        android:text="VG"
        android:id="@+id/buttonvg"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="onClickWebView1"/>

    <Button
        android:layout_width="330px"
        android:layout_height="200px"
        android:text="DB"
        android:id="@+id/buttondb"
        android:layout_above="@+id/webview1"
        android:layout_toEndOf="@+id/buttonvg"
        android:onClick="onClickWebView2"/>

    <Button
        android:layout_width="330px"
        android:layout_height="200px"
        android:text="TK"
        android:id="@+id/buttontk"

        android:onClick="onClickWebView3"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/buttondb"/>


</RelativeLayout>






